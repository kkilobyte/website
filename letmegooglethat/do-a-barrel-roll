    <script type="text/javascript">
      function GoSearch() {

        var Keywords = document.getElementById("search-input").value;
        window.location.href = "https://gprivate.com/search/index.html?q=#gsc.q=" + Keywords;

      }
    </SCRIPT>



    <SCRIPT TYPE="text/javascript">
      $(function () {

        $("form").on("submit", function (e) {
          e.preventDefault();
        });

        $("input").on("keyup", function (e) {
          if (e.keyCode === 13) {
            //the functions have to execute in this order

            GoSearch();

          }
        });

      });

    </script>

